$("#day_part_meal_<%= @day_part_meal.id %>").html("Zjedzono :)");

<% day_part = @day_part_meal.day_part %>
$("#day_part_<%= day_part.id %>").html(
    "<%= j (render(
                  partial: 'day_parts/day_part',
                  locals: {day_part: day_part}
              )) %>");

<% day = day_part.day %>
$("#month_day_<%= day.date.strftime("%d_%m_%Y") %>").html("<%= j (render(
                    partial: "days/month_day",
                    locals: {day: day.date.to_date, start_date: day.date.to_date, user: day.patient}
                )) %>");